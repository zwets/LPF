{
  "name": "moss",
  "version": "1.1.0",
  "description": "Microbial Outbreak Surveillance System",
  "main": "main.js",
  "scripts": {
    "dev": "npm run development",
    "start": "electron .",
    "dist": "electron-builder"
  },
